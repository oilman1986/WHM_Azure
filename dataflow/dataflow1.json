{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "NoviWellCSV",
						"type": "DatasetReference"
					},
					"name": "NoviWellCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Novi_Well_Table",
						"type": "DatasetReference"
					},
					"name": "NoviWellTempSQL"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Row as string,",
				"          API as string,",
				"          {Well Name} as string,",
				"          {Current Operator} as string,",
				"          {Original Operator} as string,",
				"          Basin as string,",
				"          Subbasin as string,",
				"          State as string,",
				"          County as string,",
				"          Formation as string,",
				"          Field as string,",
				"          {Well type} as string,",
				"          {Lateral Length} as string,",
				"          {First Completion Proppant Mass} as string,",
				"          {Frac jobs} as string,",
				"          {Spud Date} as string,",
				"          {Drilling Completion Date} as string,",
				"          {First Production Date} as string,",
				"          {Plugged Date} as string,",
				"          TVD as string,",
				"          MD as string,",
				"          {Well Latitude} as string,",
				"          {Well Longitude} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'FileName',",
				"     wildcardPaths:['novi_wells*']) ~> NoviWellCSV",
				"NoviWellCSV sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Row as integer,",
				"          API as long,",
				"          {Well Name} as string,",
				"          {Current Operator} as string,",
				"          {Original Operator} as string,",
				"          Basin as string,",
				"          Subbasin as string,",
				"          State as string,",
				"          County as string,",
				"          Formation as string,",
				"          Field as string,",
				"          {Well type} as string,",
				"          {Lateral Length} as integer,",
				"          {First Completion Proppant Mass} as integer,",
				"          {Frac jobs} as integer,",
				"          {Spud Date} as date,",
				"          {Drilling Completion Date} as date,",
				"          {First Production Date} as date,",
				"          {Plugged Date} as date,",
				"          TVD as integer,",
				"          MD as integer,",
				"          {Well Latitude} as float,",
				"          {Well Longitude} as float,",
				"          {Column 23} as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> NoviWellTempSQL"
			]
		}
	}
}